using System;
using System.Collections.Generic;
using System.Web;
using System.Web.UI;
using System.Web.UI.WebControls;
using LitJson;
using HDAPI;

public partial class sample_information : System.Web.UI.Page
{
    protected void Page_Load(object sender, EventArgs e)
    {
        HDAPI.HDBase hd = new HDAPI.HDBase(Request);
        Response.Write("<b>Information about Nokia N95</b> <br>");
        if (hd.information("Nokia","n95"))
        {
            JsonData data = hd.getInformation();
            foreach (System.Collections.Generic.KeyValuePair<string,JsonData> o in data)
            {
                Response.Write(o.Key+":"+o.Value.ToJson() + "<br>");
            }
        }
        else
        {
            Response.Write("Regular request");
        }
    }
}
