using System;
using System.Data;
using System.Configuration;
using System.Collections;
using System.Web;
using System.Web.Security;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Web.UI.WebControls.WebParts;
using System.Web.UI.HtmlControls;


using HDAPI;
using LitJson;

public partial class sample_vendor : System.Web.UI.Page
{
    protected void Page_Load(object sender, EventArgs e)
    {
        HDBase hd = new HDBase(Request);

        if (hd.vendor())
        {
            Response.Write("<b>List of all vendors</b> <br>");
            JsonData data = hd.getVendor();
            foreach (object o in data["vendor"])
            {
                Response.Write(Convert.ToString(o.ToString()) + "<br/>");
            }
        }
        else
        {
            Response.Write("Could not fetch all vendors");
        }
    }
}
