<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="username" value="**your api username**"/>
    <add key="secret" value="**your api secret**"/>
    <add key="site_id" value="**your api siteId**"/>
    <add key="aspnet:MaxJsonDeserializerMembers" value="100000" />
    <add key="use_local" value="true"/>
  </appSettings>
  <system.web.extensions>
    <scripting>
      <webServices>
        <jsonSerialization maxJsonLength="20000000" />
      </webServices>
    </scripting>
  </system.web.extensions>
</configuration>

